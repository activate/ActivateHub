%h1 Editing organization

= semantic_form_for(@organization) do |f|
  = f.inputs do
    = f.input :name
    = f.input :contact_name
    = f.input :email
    = f.input :url
    = f.input :description
    = f.input :topics, :input_html => { :class => 'required chzn-select-create' }
  = f.buttons do
    = f.submit "Update"
